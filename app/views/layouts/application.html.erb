  <!-- <body> -->

    <div class="site-wrapper">

      <div class="site-wrapper-inner">

        <div class="cover-container">
				<div class="masthead clearfix">
					<div class="inner">
						<h3 class="masthead-brand" id="company_name">Catalyst Business Centre</h3>
						<ul class="nav masthead-nav" id="top_nav">
							<li>		
								<form class="form-search" method="get" id="s" action="/">
							    <div class="input-append">
							        <input type="text" class="input-medium search-query" name="s" placeholder="Search by form number" value="" style="color: black;">
							    </div>
								</form>
							</li>
							<li><a href="#">Home</a></li>
							<li><a href="#">Contact</a></li>
						</ul>
					</div>
				</div>
				<%= yield %>
				 <div class="mastfoot">
            <div class="inner">
              <p>Copyrights to <a href="#">Catalyst Forms</a>, by <a href="#">@Catalyst</a>.</p>
            </div>
          </div>

        </div>

      </div>

    </div>

			</body>
			</html>

